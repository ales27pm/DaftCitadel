{
  "name": "daft-citadel-audio",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "test": "npx --yes jest",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --write \"{src,docs}/**/*.{ts,tsx,md}\"",
    "manage:agents": "node scripts/manageAgents.js"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.25",
    "@shopify/react-native-skia": "^1.4.2",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "^2.14.1",
    "react-native-reanimated": "^3.7.1",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "^3.29.0"
  },
  "devDependencies": {
    "@react-native/eslint-config": "^0.73.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.16.3",
    "@types/react": "^18.2.21",
    "@types/react-native": "^0.73.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "5.5.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native|@react-native|react-native-.*|@react-navigation)/)"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "moduleNameMapper": {
      "^react-native$": "<rootDir>/__mocks__/react-native.ts",
      "^react-native-reanimated$": "<rootDir>/__mocks__/react-native-reanimated.ts",
      "^@shopify/react-native-skia$": "<rootDir>/__mocks__/@shopify/react-native-skia.ts"
    }
  }
}
